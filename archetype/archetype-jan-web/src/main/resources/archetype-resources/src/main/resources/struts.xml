<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- Designated by the spring action is responsible for the creation of 
		an object -->
	<constant name="struts.objectFactory" value="spring" />
	<!-- All matching *. Action request by struts2 processing -->
	<constant name="struts.action.extension" value="action" />
	<!-- Whether enable development mode -->
	<constant name="struts.devMode" value="true" />
	<!-- Struts configuration file changes, whether or not to load -->
	<constant name="struts.configuration.xml.reload" value="true" />
	<!-- Set whether the browser cache static content -->
	<constant name="struts.serve.static.browserCache" value="false" />
	<!-- Request parameter coding mode -->
	<constant name="struts.i18n.encoding" value="utf-8" />
	<!-- Every time an HTTP request system to load a resource file, help to 
		develop -->
	<constant name="struts.i18n.reload" value="true" />
	<!-- File upload maximum -->
	<constant name="struts.multipart.maxSize" value="104857600" />
	<!-- Let struts2 support dynamic method invocation -->
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
	<!-- Whether the name of Action or diagonal -->
	<constant name="struts.enable.SlashesInActionNames" value="false" />
	<!-- Allowed tags used expression grammar -->
	<constant name="struts.tag.altSyntax" value="true" />
	<!-- For WebLogic, Orion, OC4J this attribute should be set to true -->
	<constant name="struts.dispatcher.parametersWorkaround" value="false" />

</struts>