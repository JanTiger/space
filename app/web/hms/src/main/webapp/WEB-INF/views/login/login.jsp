<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><%@ taglib uri="http://java.sun.com/jstl/core_rt" prefix="c" %><%@ page contentType="text/html;charset=UTF-8" language="java" pageEncoding="UTF-8"%><head>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>    <title>Login - hms</title>    <link href="resources/css/login.css" rel="stylesheet" type="text/css"/>    <script type="text/javascript" src="resources/script/jquery-easyui-1.3.1/jquery-1.8.0.min.js" charset="utf-8"></script>    <script type="text/javascript">	    function loginListen(event) {	        if (event.keyCode == 13) {	        	login();	        }	    }	    function login(){		    if(isEmpty($("#userName")) || isEmpty($("#password")))			    return null;		    $("#loginForm").submit();	    }	    function isEmpty(obj){		    if(obj.val() == ''){			    obj.focus();			    return true;		    }		    return false;		}    </script></head><body class="bg_class" onkeydown="loginListen(event);"><div class="wrapper">    <div class="content_class">        <div class="logo_class"></div>        <div class="title_class"></div>        <div id="errorBox">             <c:if test="${errorMsg != null}">                 <label>${errorMsg}</label>             </c:if>        </div>        <form id="loginForm" action="userAction!login.action" method="post">            <hr />            <fieldset>            <div class="field-group">                <label id="username-label" for="userName">UserName</label>                <input name="userName" class="text" id="userName" type="text" />            </div>            <div class="field-group">                <label id="password-label" for="password">Password</label>                <input name="password" class="text" id="password" type="password" />            </div>            </fieldset>            <hr />            <div id="btnDiv">                <a href="#" id="login_button" onclick="login();" class="login_button origin-ux-element origin-ux-button origin-ux-button-tertiary">                    <span><span>Login</span></span>                </a>            </div>        </form>    </div></div></body></html>