<%@ taglib uri="http://java.sun.com/jstl/core_rt" prefix="c" %><%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn" %><%@ page contentType="text/html;charset=UTF-8" language="java" pageEncoding="UTF-8"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>    <title>Login - hms</title>    <link href="resources/css/login.css" rel="stylesheet" type="text/css"/>    <script type="text/javascript" src="resources/script/jquery/jquery-1.8.3.min.js"></script>    <script type="text/javascript">    function login(event) {        if (event.keyCode == 13) {            document.forms[0].submit();        }    }    </script></head><body class="bg_class" onkeydown="login(event);"><div class="wrapper">    <div class="content_class">        <div class="logo_class"></div>        <div class="title_class">        </div>        <div id="errorBox">                <c:if test="${errorMsg!= null}">                    <label>${errorMsg}</label>                </c:if>            </div>        <form name="form" action="login" method="post">            <hr />            <fieldset>            <div class="field-group">                <label id="username-label" for="username">Username</label>                <input name="username" class="text   " id="username" type="text" data-focus="0"/>            </div>            <div class="field-group">                <label id="password-label" for="password">Password</label>                <input name="password" class="text" id="password" type="password" data-focus="0"/>            </div>            </fieldset>            <hr />            <div id="btnDiv">                <a href="#" id="login_button" onClick="document.forms[0].submit();"                   class="login_button origin-ux-element origin-ux-button origin-ux-button-tertiary">                    <span><span>Login</span></span>                </a>            </div>        </form>    </div></div></body></html>